{
  "version": 2,
  "name": "skylight-merged-rota",
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/rota.html",
      "dest": "/public/rota.html"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/merged-rota",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
